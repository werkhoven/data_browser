# Environment Variables Example
# Copy this file to .env and fill in your actual values

# OpenAI API Key for AI-powered features
OPENAI_API_KEY=your_openai_api_key_here

# AWS Configuration for Pipelines Service
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1
S3_BUCKET=data-browser-uploads

# Pipelines API Configuration
API_TITLE=Data Pipeline Service
API_VERSION=1.0.0
HOST=0.0.0.0
PORT=8000
MAX_FILE_SIZE_MB=100

# Pipelines Service URL (used by browser)
PIPELINES_URL=http://pipelines-api:8000

# Browser Application Configuration
BROWSER_HOST=0.0.0.0
BROWSER_PORT=8001
BROWSER_SHARE=false

# Default File Path for Browser
GRADIO_TEMP_DIR=app/data/

# LocalStack Configuration
LOCALSTACK_VOLUME_DIR=./volume
